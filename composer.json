{
    "name": "dadapas/http-client",
    "description": "The psr-18 implementation for HTTP Clients",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dadapas\\Http\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dadapas\\HttpTests\\": "tests/"
        }
    },
    "provide": {
        "psr/http-client-implementation": "1.0"
    },
    "authors": [
        {
            "name": "TOVOHERY",
            "email": "tovoherypascal@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "psr/http-client": "1.0.x-dev"
    },
    "version": "1.0.0",
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit ."
        ]
    }
